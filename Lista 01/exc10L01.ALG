Algoritmo "semnome"
// Calcule e apresente o volume de uma esfera: Volume = (4/3) * 3,1416 * R3
Var
   volume, raio : real

Inicio
      escreva("Digite o raio da esfera: ")
      leia(raio)

      volume <- (4/3) * 3,1416 * R^3
      
      escreval("O volume da esfera é: ", volume:0:2)
Fimalgoritmo