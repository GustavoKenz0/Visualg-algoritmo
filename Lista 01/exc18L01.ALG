Algoritmo "semnome"
// Solicite um número inteiro
//e apresente o resto da divisão por 2 (usar operador MOD ou %).
Var
   num,n2 : inteiro


Inicio
      escreva("Digite um numero inteiro: ")
      leia(num)
      
      n2 <- num%2
      
      escreva("O resto da divisão de ", num, " é ", n2)


Fimalgoritmo